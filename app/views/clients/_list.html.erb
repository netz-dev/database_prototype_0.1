<%# app/views/students/_list.html.erb %>
<div id="filterrific_results">



  <table class="table table-hover table-responsive-md" cellspacing="0" width="100%">
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Country</th>
      <th>Registered at</th>
    </tr>
    <% clients.each do |client| %>
      <tr>
        <td><%= link_to(client.first_name, client_path(client)) %></td>
        <td><%= client.email %></td>
        <td><%= client.contact_level %></td>
        <td><%= client.created_at %></td>
      </tr>
    <% end %>
  </table>
</div>
